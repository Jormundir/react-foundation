{
  "name": "my-express-react",
  "version": "1.0.0",
  "description": "Express + React",
  "main": "index.js",
  "scripts": {
    "start": "NODE_PATH=./app node ./babel.server.js",
    "develop": "start dev and node server",
    "bundle": "create production bundle",
    "test": "NODE_PATH=./app jest"
  },
  "author": "Charles Bryan",
  "license": "UNLICENSED",
  "dependencies": {
    "babel-core": "^6.1.21",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "ejs": "^2.3.4",
    "express": "^4.13.3",
    "history": "^1.13.1",
    "react": "^0.14.2",
    "react-dom": "^0.14.2",
    "react-router": "^1.0.0"
  },
  "devDependencies": {
    "babel-jest": "^6.0.1",
    "babel-loader": "^6.2.0",
    "jest-cli": "^0.7.1",
    "webpack": "^1.12.6",
    "webpack-dev-server": "^1.14.0"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "node"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/fbjs"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ]
  }
}
