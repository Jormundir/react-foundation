{
  "name": "my-express-react",
  "version": "0.1.0",
  "description": "Express + React",
  "repository": "https://github.com/Jormundir/react-foundation.git",
  "main": "index.js",
  "scripts": {
    "start": "node ./build/server/app.js",
    "develop": "node ./node_modules/concurrently/src/main.js --kill-others \"node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --hot --inline --config ./tools/webpack.config.js\" \"./node_modules/nodemon/bin/nodemon.js ./build/server/app.js\"",
    "test": "NODE_PATH=./app jest"
  },
  "author": "Charles Bryan",
  "license": "UNLICENSED",
  "dependencies": {
    "babel-core": "^6.1.21",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "css-loader": "^0.23.0",
    "ejs": "^2.3.4",
    "express": "^4.13.3",
    "fs-extra": "^0.26.2",
    "history": "^1.13.1",
    "react": "^0.14.2",
    "react-dom": "^0.14.2",
    "react-router": "^1.0.0",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0"
  },
  "devDependencies": {
    "async": "^1.5.0",
    "babel-cli": "^6.3.17",
    "babel-jest": "^6.0.1",
    "babel-loader": "^6.2.0",
    "css-loader": "^0.23.0",
    "extract-text-webpack-plugin": "^0.9.1",
    "file-loader": "^0.8.5",
    "jest-cli": "^0.7.1",
    "lodash": "^3.10.1",
    "node-sass": "^3.4.2",
    "nodemon": "^1.8.1",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.6",
    "webpack-dev-server": "^1.14.0"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "node"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/fbjs"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ]
  }
}
